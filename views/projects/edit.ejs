<%- include('../partials/header.ejs') %> 
<div class="container">

    <form action="/users/<%=userId%>/projects/<%=project._id%>?_method=PUT"  method="POST">
        <div class="form-group">
            <label for="title">Title:</label>
            <input class="form-control" id="title" name="title" type="text" value="<%= project.title%>" >
        </div>
        <div class="form-group">
        <label for="thumbnail">Thumbnail:</label>
            <input class="form-control" id="thumbnail>" name="thumbnail" type="text" value="<%= project.thumbnail%>" >
        </div>
    <div class="form-group">
        <label for="url">Url:</label>
        <input class="form-control" id="url" name="url" type="text" value="<%= project.url %>" >
    </div>
    <input class="btn btn-outline-success" type="submit" >
    
</form>
</div>
<%- include('../partials/footer.ejs') %>